{
  "generator-fireloop": {
    "version": "1.0.0-beta.2.5",
    "clients": {
      "fireloop": {
        "path": "./fireloop",
        "type": "server"
      },
      "webapp": {
        "path": "./webapp",
        "type": "ng2web",
        "name": "webapp",
        "sdkPath": "webapp\\src\\app\\shared\\sdk"
      }
    },
    "promptValues": {
      "sdkFeatures": [
        "Generate ONLY FireLoop SDK + Auth Services"
      ]
    }
  }
}