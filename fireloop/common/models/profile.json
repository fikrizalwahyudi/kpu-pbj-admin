{
  "name": "profile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for profile registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "id": {
      "type": "number",
      "required": true
    },
    "nama": {
      "type": "string"
    },
    "tempat_lahir": {
      "type": "string"
    },
    "tgl_lahir": {
      "type": "date"
    },
    "alamat": {
      "type": "string"
    },
    "id_pendidikan_terakhir": {
      "type": "number"
    },
    "photo": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "pendidikan_terakhir": {
      "type": "belongsTo",
      "model": "pendidikan_terakhir",
      "foreignKey": "id_pendidikan_terakhir"
    }
  },
  "acls": [],
  "methods": {}
}
